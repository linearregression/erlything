{% extends "base_without.dtl" %}

{% block title %}Gallery{% endblock %}

{% block body %}

<div class="container-fluid">
    <div class="row">
		{%for photo, date in photos%}
			<div class="col-xs-3">
				<img src="/photos/{{photo}}" class="img-rounded">{{date|linebreaks}}
			</div>
		{%endfor%}
	</div>

</div>
{% endblock %}