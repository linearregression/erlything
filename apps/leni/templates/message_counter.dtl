{% extends "base_without.dtl" %}

{% block title %}Messages{% endblock %}

{% block body %}


<div class="panel panel-primary">
    <div class="panel-heading">
        <h2 class="panel-title">Message count : {{counter}}</h2>  
    </div>
    <div class="panel-body">

<table class="table">
	<thead>
		<tr>
			<th>Time</th>
			<th>Node</th>
			<th>Thing</th>
			<th>Id</th>
			<th>Time</th>
			<th>Payload</th>
		</tr>	
	</thead>
	<tbody>
	{% for date, message in data %}
		<tr>
			<td>{{date}}</td>
			{% for detail in message%}
			<td>{{detail}}</td>
			{% endfor %}
		</tr>
	{% endfor %}
	</tbody>
</table>
</div>
<div class="panel-footer"></div>
</div>

{% endblock %}